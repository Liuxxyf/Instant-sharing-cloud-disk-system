AC,cADA;AACA;AACAC,4CAFA;AAEA;AACAhB,mBAHA;AAGA;AACAiB,qBAJA,CAIA;;AAJA,OADA,EAOA;AACAF,cADA;AACA;AACAC,4CAFA;AAEA;AACAhB,qBAHA;AAGA;AACAiB,qBAJA,CAIA;;AAJA,OAPA,EAaA;AACAF,cADA;AACA;AACAC,4CAFA;AAEA;AACAhB,qBAHA;AAGA;AACAiB,qBAJA,CAIA;;AAJA,OAbA,EAJA,CAwBA;;AACAC;AACAX;AADA,SAGAY,IAHA,CAGA;AACAC,yBADA,CAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,OAbA,EAcAC,KAdA,CAcA;AACAD;AACA,OAhBA;AAkBA,KA5CA;AA6CAE,eA7CA,uBA6CAC,IA7CA,EA6CA;AACAA;AACA,KA/CA;AAgDAC,QAhDA,kBAgDA;AACA;AACA;AACAC,+BADA;AAEAC,8BAFA;AAGAC,sBACA,8EAJA;AAKAC;AALA,SAOAT,IAPA,CAOA;AAAA;AACAU;AACAC,yBADA;AAEAC;AAFA;AAIAC;AACAzB;AADA,WAEAY,IAFA,CAEA;AACAC;AACA,SAJA,EAIAC,KAJA,CAIA;AACAD;AACA,SANA;AAOA,OAnBA,EAoBAC,KApBA,CAoBA,cApBA;AAqBA,KAvEA;AAwEAY,iBAxEA,2BAwEA;AACA;AACAC;AACA;AACAC;AACA,KA7EA;AA8EAC,oBA9EA,4BA8EAC,KA9EA,EA8EA;AACA;AACAH;AACA;AACAC;AACAG;AACAP,uCADA;AAEAf;AAFA;AAIA,KAvFA;AAwFAuB,gBAxFA,0BAwFA;AACA;;AACA;AACAC;AACA,OAFA,MAEA;AACAA;AACAF;AACA;AACAG,yBADA;AAEAV;AAFA;AAIA;AACA,KApGA;AAqGAW,QArGA,kBAqGA;AACA;AACAC;AACA;AAxGA,GApBA;AA8HAC,SA9HA,qBA8HA;AAAA;;AACAN,sBADA,CACA;;AACAA,qCAFA,CAEA;;AACAA;AACAA;AACA;AACA;AACA,KAHA;AAIAA;AACAA;AACA;AACA,KAFA;AAGA;AA1IA","names":["name","data","drawer","direction","nickname","avatarUrl","friendlist","token","components","Friend","Chatbox","Myfriendlist","methods","showchart","chart","id","imgurl","time","GetFriendChartAPI","then","console","catch","handleClose","done","open","confirmButtonText","cancelButtonText","inputPattern","inputErrorMessage","that","type","message","GetFriendRequestAPI","friendmessage","friend","chatbox","chartbox_message","index","bus","myfriendList","myfriend_list","showClose","hide","myfriendlist","mounted"],"sourceRoot":"src/views","sources":["Chart.vue"],"sourcesContent":["<template>\n  <div class=\"chart\" @click=\"hide\">\n    <el-drawer\n      :visible.sync=\"drawer\"\n      :direction=\"direction\"\n      size=\"100%\"\n      modal:false\n      :before-close=\"handleClose\"\n    >\n      <div class=\"center\">\n        <div class=\"left\">\n          <div class=\"left_top\">\n            <img :src=\"avatarUrl\" alt=\"\" class=\"left_top_img\" />\n            <div class=\"nickname\">\n              <p>{{ nickname }}</p>\n            </div>\n            <img\n              src=\"../img/friendList.png\"\n              class=\"friendList\"\n              @click.stop=\"myfriendList\"\n            />\n          </div>\n          <div class=\"left_center\">\n            <div\n              class=\"item\"\n              v-for=\"(item, index) in friendlist\"\n              :key=\"index + 1\"\n              @click=\"chartbox_message(index)\"\n            >\n              <img :src=\"item.imgurl\" alt=\"\" />\n              <p class=\"name\">{{ item.name }}</p>\n              <p class=\"time\">{{ item.time }}</p>\n              <p class=\"chart_message\">{{ item.message }}</p>\n            </div>\n          </div>\n          <div class=\"left_bottom\">\n            <div class=\"left_first_circle\" title=\"添加好友\" @click=\"open\"></div>\n            <div\n              class=\"left_second_circle\"\n              title=\"好友信息\"\n              @click=\"friendmessage\"\n            >\n              <img src=\"../img/warn.png\" />\n            </div>\n          </div>\n        </div>\n        <div class=\"right\">\n          <div class=\"chart_icon\"></div>\n        </div>\n        <Friend class=\"friend\"></Friend>\n        <Chatbox class=\"Chat-box\"></Chatbox>\n        <Myfriendlist class=\"myfriend-list\"></Myfriendlist>\n      </div>\n    </el-drawer>\n  </div>\n</template>\n<script>\n//聊天页面\nimport bus from \"../utils/bus\";\nimport Chatbox from \"./Chat_box.vue\";\nimport Friend from \"./Friend.vue\";\nimport Myfriendlist from \"./Myfriendlist.vue\";\nimport { GetFriendChartAPI, GetFriendRequestAPI } from \"../request/api\";\nexport default {\n  name: \"Chart\",\n  data() {\n    return {\n      drawer: false,\n      direction: \"rtl\",\n      nickname: \"\",\n      avatarUrl: null,\n      //用户的好友阅读信息\n      friendlist: [],\n      //token\n      token: null,\n    };\n  },\n  components: {\n    Friend,\n    Chatbox,\n    Myfriendlist,\n  },\n\n  methods: {\n    showchart() {\n      let chart = document.getElementsByClassName(\"chart\")[0];\n      chart.style.display = \"block\";\n      this.drawer = true;\n      this.friendlist = [\n        {\n          id: 10, //好友id\n          imgurl: require(\"../img/avatar.jpg\"), //好友头像\n          name: \"liu\", //好友名称\n          time: \"14:30\", //好友时间\n        },\n        {\n          id: 10, //好友id\n          imgurl: require(\"../img/avatar.jpg\"), //好友头像\n          name: \"test1\", //好友名称\n          time: \"13:11\", //好友时间\n        },\n        {\n          id: 10, //好友id\n          imgurl: require(\"../img/avatar.jpg\"), //好友头像\n          name: \"text2\", //好友名称\n          time: \"11:45\", //好友时间\n        },\n      ];\n      //axios请求获取好友聊天列表\n      GetFriendChartAPI({\n        token:localStorage.getItem(\"token\")\n      })\n        .then(function (res) {\n          console.log(res);\n          // this.friendlist = [\n          //   {\n          //     id: 10,//好友id\n          //     imgurl:  //好友头像\n          //     name: \"liu999999\", //好友名称\n          //     time: \"17:11\",  //好友时间\n          //   },\n          // ];\n        })\n        .catch(function (err) {\n          console.log(err);\n        });\n       \n    },\n    handleClose(done) {\n      done();\n    },\n    open() {\n      let that = this;\n      this.$prompt(\"请输入手机号\", \"添加好友\", {\n        confirmButtonText: \"发送\",\n        cancelButtonText: \"取消\",\n        inputPattern:\n          /^(13[0-9]|14[01456879]|15[0-35-9]|16[2567]|17[0-8]|18[0-9]|19[0-35-9])\\d{8}$/,\n        inputErrorMessage: \"手机格式不正确\",\n      })\n        .then(({ value }) => {\n          that.$message({\n            type: \"success\",\n            message: \"发送成功~等待对方同意\",\n          });\n          GetFriendRequestAPI({\n             token: localStorage.getItem(\"token\"),\n          }).then(function(res){\n            console.log(res)\n          }).catch(function(err){\n            console.log(err)\n          })\n        })\n        .catch(() => {});\n    },\n    friendmessage() {\n      let friend = document.getElementsByClassName(\"friend\")[0];\n      friend.style.display = \"block\";\n      let chatbox = document.getElementsByClassName(\"chatbox\")[0];\n      chatbox.style.display = \"none\";\n    },\n    chartbox_message(index) {\n      let friend = document.getElementsByClassName(\"friend\")[0];\n      friend.style.display = \"none\";\n      let chatbox = document.getElementsByClassName(\"chatbox\")[0];\n      chatbox.style.display = \"block\";\n      bus.$emit(\"index\", {\n        message: this.friendlist[index],\n        imgurl: this.avatarUrl,\n      });\n    },\n    myfriendList() {\n      let myfriend_list = document.getElementsByClassName(\"myfriend-list\")[0];\n      if (myfriend_list.style.display == \"block\") {\n        myfriend_list.style.display = \"none\";\n      } else {\n        myfriend_list.style.display = \"block\";\n        bus.$emit(\"getimgurl\", this.avatarUrl);\n        this.$message({\n          showClose: true,\n          message: \"双击开启聊天~\",\n        });\n      }\n    },\n    hide() {\n      let myfriendlist = document.getElementById(\"myfriendlist\");\n      myfriendlist.style.display = \"none\";\n    },\n  },\n  mounted() {\n    bus.$off(\"chart\"); //阻止事件被触发多次\n    bus.$on(\"chart\", this.showchart); //绑定\n    bus.$off(\"chartmessage\");\n    bus.$on(\"chartmessage\", (msg) => {\n      this.nickname = msg.nickname;\n      this.avatarUrl = \"http://localhost:8080/\" + msg.avatarUrl;\n    });\n    bus.$off(\"changeuserAvatar_chart\");\n    bus.$on(\"changeuserAvatar_chart\", (msg) => {\n      this.avatarUrl = msg;\n    });\n  },\n};\n</script>\n<style scoped>\n.center {\n  position: absolute;\n  height: 90%;\n  width: 80%;\n  box-shadow: 0 0 20px -5px;\n  left: 10%;\n  top: 5%;\n}\n.left {\n  position: absolute;\n  height: 100%;\n  width: 24%;\n  border-right: 2px solid #e0e0e0;\n}\n.right {\n  position: absolute;\n  height: 100%;\n  width: calc(76% - 1px);\n  right: 0;\n}\n.left_top {\n  position: absolute;\n  height: 18%;\n  width: 100%;\n  border-bottom: 2px solid #e0e0e0;\n}\n.left_center {\n  position: absolute;\n  height: 73%;\n  top: calc(18% + 2px);\n  width: 100%;\n  border-bottom: 2px solid #e0e0e0;\n  overflow-y: auto;\n  scrollbar-color: rgba(0, 0, 0, 0.2) rgba(0, 0, 0, 0.1);\n  scrollbar-width: thin;\n  -ms-overflow-style: none;\n}\n.left_bottom {\n  position: absolute;\n  height: calc(9% - 4px);\n  bottom: 0;\n  width: 100%;\n}\n.left_first_circle {\n  position: absolute;\n  height: 40px;\n  width: 40px;\n  top: 50%;\n  left: 30%;\n  transform: translate(-50%, -50%);\n  border-radius: 40px;\n  background-image: url(\"../img/plus_friend.png\");\n  background-position: center;\n  background-size: cover;\n  background-repeat: no-repeat;\n  cursor: pointer;\n}\n.left_second_circle {\n  position: absolute;\n  height: 40px;\n  width: 40px;\n  top: 50%;\n  right: 20%;\n  transform: translate(-50%, -50%);\n  cursor: pointer;\n}\n.left_second_circle img {\n  position: absolute;\n  height: 100%;\n  width: 100%;\n}\n.left_top_img {\n  position: absolute;\n  height: 80px;\n  width: 80px;\n  top: 50%;\n  left: 20%;\n  transform: translate(-50%, -50%);\n}\n.nickname {\n  position: absolute;\n  color: dimgrey;\n  font-size: 15px;\n  height: 50%;\n  width: 30%;\n  left: 60%;\n  top: 50%;\n  transform: translate(-50%, -50%);\n}\n.nickname p {\n  position: absolute;\n  left: 50%;\n  bottom: 40%;\n  transform: translate(-50%, 0);\n  margin: 0;\n}\n.item {\n  position: relative;\n  height: 15%;\n  width: 100%;\n  background-color: #f5f5f5;\n  cursor: pointer;\n}\n.item:hover {\n  background-color: rgb(194, 194, 194);\n}\n.item img {\n  position: absolute;\n  height: 70%;\n  top: 15%;\n  left: 5%;\n}\n.name {\n  position: absolute;\n  top: 20%;\n  left: 35%;\n  width: 50%;\n  margin: 0;\n  font-size: 13px;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n  overflow: hidden;\n}\n.time,\n.chart_message {\n  color: rgb(134, 134, 134);\n  position: absolute;\n  right: 2%;\n  top: 20%;\n  margin: 0;\n  font-size: 13px;\n}\n.chart_message {\n  left: 35%;\n  top: 50%;\n  width: 65%;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n  overflow: hidden;\n}\n.chart_icon {\n  position: absolute;\n  height: 100px;\n  width: 100px;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  background-image: url(\"../img/chart_icon.png\");\n  background-position: center;\n  background-size: cover;\n}\n.left_center::-webkit-scrollbar {\n  width: 4px;\n  /*height: 4px;*/\n}\n.left_center::-webkit-scrollbar-thumb {\n  border-radius: 10px;\n  -webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);\n  background: rgba(0, 0, 0, 0.2);\n}\n.left_center::-webkit-scrollbar-track {\n  -webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);\n  border-radius: 0;\n  background: rgba(0, 0, 0, 0.1);\n}\n.friend {\n  display: none;\n}\n.friendList {\n  position: absolute;\n  width: 30%;\n  right: -10%;\n  bottom: 30%;\n  cursor: pointer;\n}\n.myfriend-list {\n  display: none;\n}\n</style>\n"]}]}