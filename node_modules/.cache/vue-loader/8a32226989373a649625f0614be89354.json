  },\n  methods: {\n      //页面是否登录提示信息\n    loginshowmessage() {\n        this.tologin()\n    },\n    loginsucess(){\n        this.tosucesslogin()\n    },\n    loginerror(){\n        this.toerrorlogin()\n    },\n    open1() {\n      this.$message({\n        showClose: true,\n        message: \"这是一条消息提示\",\n      });\n    },\n\n    tosucesslogin() {\n      this.$message({\n        showClose: true,\n        message: \"登录成功~\",\n        type: \"success\",\n        offset:70\n      });\n    },\n\n    tologin() {\n      this.$message({\n        showClose: true,\n        message: \"请先登录账号！\",\n        type: \"warning\",\n         center: true,\n         offset:70\n      });\n    },\n\n   toerrorlogin(){\n      this.$message({\n        showClose: true,\n        message: \"输入错误，请重新输入！\",\n        type: \"error\",\n      });\n    },\n    phone_error(){\n      this.$message({\n        showClose: true,\n        message: \"手机号错误，请重新输入！\",\n        type: \"error\",\n      });\n    }\n  },\n  mounted() {\n    bus.$off(\"loginshowmessage\"); //阻止事件被触发多次\n    bus.$on(\"loginshowmessage\", this.loginshowmessage); //绑定\n    bus.$off(\"loginsucess\"); //阻止事件被触发多次\n    bus.$on(\"loginsucess\", this.loginsucess); //绑定\n    bus.$off(\"loginerror\"); //阻止事件被触发多次\n    bus.$on(\"loginerror\", this.loginerror); //绑定\n    bus.$off(\"phone_error\"); //阻止事件被触发多次\n    bus.$on(\"phone_error\", this.phone_error); //绑定\n  },\n\n};\n</script>\n\n<style scoped>\n.popup {\n  position: relative;\n}\n</style>"]}]}